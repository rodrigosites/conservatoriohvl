<div class="page-header">
  <h1>Relatório - Folha de Pagamento</h1>
</div>
  
<div class="form-inline">
  <div class="form-group">
    <label for="folhapagamento_mes">Mês:</label>
    <%= collection_select(:folhapagamento, :mes, FolhaPagamento.select('mes').uniq, :mes, :mes, { prompt: 'Selecione o mês...' }, class: "form-control")%>
  </div>
  <div class="form-group">
    <label for="folhapagamento_ano">Ano:</label>
    <%= collection_select(:folhapagamento, :ano, FolhaPagamento.select('ano').uniq, :ano, :ano, { prompt: 'Selecione o mês...' }, class: "form-control")%>
  </div>
  <div class="form-group">
    <button id="button" class="btn btn-primary btn-sm" disabled="true">Visualizar</button>
  </div>
</div>
<br>
<div id = "conteudo">
</div>